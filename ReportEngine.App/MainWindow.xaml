<Window
    x:Class="ReportEngine.App.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ReportEngine.App"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <Grid>
        <Button x:Name="HighQualityButton" Height="50" Width="200" Content="Set high quality HMI" Margin="10,374,590,10" Click="High_button_click" />
        <Button x:Name="LowQualityButton" Height="50" Width="200" Content="Set low quality HMI" Margin="590,374,10,10" Click="Low_button_click"/>
        <Label x:Name="textDisplay" Height="50" Content="HMI quality is so unknown" FontSize="20" Margin="210,192,270,192" />
    </Grid>
</Window>
